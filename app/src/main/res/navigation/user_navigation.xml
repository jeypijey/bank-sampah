<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/user_navigation"
    app:startDestination="@id/userHomeFragment">
    <fragment
        android:id="@+id/userHomeFragment"
        android:name="com.d3if0104.banksampah.fragment.UserHomeFragment"
        android:label="fragment_user_home"
        tools:layout="@layout/fragment_user_home" >
        <action
            android:id="@+id/action_userHomeFragment_to_userProfileFragment"
            app:destination="@id/userProfileFragment" />
        <action
            android:id="@+id/action_userHomeFragment_to_userRiwayatSampahFragment"
            app:destination="@id/userRiwayatSampahFragment" />
        <action
            android:id="@+id/action_userHomeFragment_to_userRiwayatTarikSaldoFragment"
            app:destination="@id/userRiwayatTarikSaldoFragment" />
    </fragment>
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.d3if0104.banksampah.fragment.UserProfileFragment"
        android:label="fragment_user_profile"
        tools:layout="@layout/fragment_user_profile" >
        <action
            android:id="@+id/action_userProfileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.d3if0104.banksampah.fragment.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/userRiwayatSampahFragment"
        android:name="com.d3if0104.banksampah.fragment.UserRiwayatSampahFragment"
        android:label="fragment_user_riwayat_sampah"
        tools:layout="@layout/fragment_user_riwayat_sampah" />
    <fragment
        android:id="@+id/userRiwayatTarikSaldoFragment"
        android:name="com.d3if0104.banksampah.fragment.UserRiwayatTarikSaldoFragment"
        android:label="fragment_user_riwayat_tarik_saldo"
        tools:layout="@layout/fragment_user_riwayat_tarik_saldo" />
</navigation>